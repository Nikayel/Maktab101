{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "jsx": "react-jsx",
      "allowJs": true,
      "outDir": "./dist"
    },
    "include": ["src", "server"],
    "exclude": ["node_modules"]
  }
  
  